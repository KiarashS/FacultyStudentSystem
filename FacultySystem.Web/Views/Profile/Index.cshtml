    <div class="uk-grid mb-60 section-in-mobile">
        <div class="uk-width-1-1 uk-container uk-container-center">
            <div class="icon-container" style="background-color: #2196F3 !important;" data-uk-tooltip="{pos:'top', cls:'fs-16p'}" title="مشخصات">
                <i class="material-icons md-48 white" style="margin-top: 25%;">details</i>
            </div>
            <div class="md-card">
                <div class="md-card-content large-padding pt-70 bg-blue">
                    <div class="uk-grid uk-grid-divider uk-grid-medium uk-margin-medium-top uk-margin-large-bottom" data-uk-grid-margin="">
                        <div class="uk-width-large-1-2 uk-width-medium-1-2">
                            <ul class="md-list md-list-addon">
                                @if (!string.IsNullOrEmpty(Model.Fullname))
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">face</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.Fullname</span>
                                                <span class="uk-text-small silver">نام و نام خانوادگی</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (Model.BirthDate != null)
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">cake</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading persian-number white">@Model.JalaliBirthDate</span>
                                                <span class="uk-text-small silver">تاریخ تولد</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (!string.IsNullOrEmpty(Model.Location))
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">location_city</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.Location</span>
                                                <span class="uk-text-small silver">محل سکونت</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (Model.Sex != Sex.NotDefined)
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">all_out</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.SexText</span>
                                                <span class="uk-text-small silver">جنسیت</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (Model.EducationalDegree != "--")
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">stars</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.EducationalDegree</span>
                                                <span class="uk-text-small silver">درجه تحصیلی</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                <li>
                                    <div class="md-list-addon-element">
                                        <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">email</i>
                                    </div>
                                    <div class="md-list-content">
                                        <span class="md-list-heading"><img src="~/ImageGenerator.ashx?Code=@RijndaelManagedEncryption.EncryptRijndael(Model.Email)&FontFamily=Calibri&FontSize=13&TextColor=White&BackgroundColor=Transparent&UsePng=True" class="cursor-pointer" alt="ایمیل @Model.Fullname" /></span>
                                        <span class="uk-text-small silver">ایمیل اصلی</span>
                                    </div>
                                </li>
                                @if (!string.IsNullOrEmpty(Model.PersonalWebPage))
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">home</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading"><a href="@Model.PersonalWebPage" target="_blank" class="font-arial white yellowhover">@Model.PersonalWebPage</a></span>
                                                <span class="uk-text-small silver">سایت شخصی</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                            </ul>
                        </div>
                        <div class="uk-width-large-1-2 uk-width-medium-1-2">
                            <ul class="md-list md-list-addon">
                                @if (!string.IsNullOrEmpty(Model.CommonAuthorPaperName))
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">assignment_ind</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.CommonAuthorPaperName</span>
                                                <span class="uk-text-small silver">نام متعارف در مقالات</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (Model.College != "--")
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">school</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.College</span>
                                                <span class="uk-text-small silver">دانشکده</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (!string.IsNullOrEmpty(Model.BirthPlace))
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">location_on</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.BirthPlace</span>
                                                <span class="uk-text-small silver">محل تولد</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (Model.MaritalStatus != MaritalStatus.NotDefined)
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">group</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.MaritalStatusText</span>
                                                <span class="uk-text-small silver">وضعیت تاهل</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (Model.EducationalGroup != "--")
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">group_work</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.EducationalGroup</span>
                                                <span class="uk-text-small silver">گروه آموزشی</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (Model.AcademicRank != "--")
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">check_circle</i>
                                            </div>
                                            <div class="md-list-content">
                                                <span class="md-list-heading white heading_a">@Model.AcademicRank</span>
                                                <span class="uk-text-small silver">مرتبه علمی</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                                @if (!string.IsNullOrEmpty(Model.Mobile))
                                {
                                    <text>
                                        <li>
                                            <div class="md-list-addon-element">
                                                <i class="md-list-addon-icon material-icons" style="color: #FFDC00 !important;">phone_iphone</i>
                                            </div>
                                            <div class="md-list-content">
                                                @*<span class="md-list-heading"><a href="tel:@Model.Mobile" class="persian-number white yellowhover">@Model.Mobile</a></span>*@
                                                <span class="md-list-heading"><img src="~/ImageGenerator.ashx?Code=@RijndaelManagedEncryption.EncryptRijndael(Model.Mobile)&FontFamily=Calibri&FontSize=13&TextColor=White&BackgroundColor=Transparent&UsePng=True" class="cursor-pointer" alt="موبایل @Model.Fullname" /></span>
                                                <span class="uk-text-small silver">شماره همراه</span>
                                            </div>
                                        </li>
                                    </text>
                                }
                            </ul>
                        </div>
                    </div>

                    @if (Model.HasFreeField)
                    {
                        Html.RenderAction(MVC.Profile.FreeFields());
                    }

                    @if (!string.IsNullOrEmpty(Model.SecondaryEmails) ||
 !string.IsNullOrEmpty(Model.ResearchFields) ||
 !string.IsNullOrEmpty(Model.Interests))
                    {
                        <text>
                            <hr class="uk-grid-divider" />
                            <div class="uk-grid uk-margin-medium-top uk-margin-large-bottom uk-grid-divider" data-uk-grid-margin="">
                                @if (!string.IsNullOrEmpty(Model.SecondaryEmails))
                                {
                                    <div class="uk-width-small-1-1 uk-width-medium-1-3 uk-width-large-1-3 uk-width-xLarge-1-3">
                                        <h4 class="heading_c uk-margin-small-bottom silver">آدرس های ایمیل جایگزین</h4>
                                        <ul>
                                            @foreach (var item in Model.SecondaryEmailsList)
                                            {
                                                <li class="white"><img src="~/ImageGenerator.ashx?Code=@RijndaelManagedEncryption.EncryptRijndael(item)&FontFamily=Calibri&FontSize=13&TextColor=White&BackgroundColor=Transparent&UsePng=True" class="cursor-pointer" alt="ایمیل فرعی @Model.Fullname" /></li>
                                            }
                                        </ul>
                                    </div>
                                }
                                @if (!string.IsNullOrEmpty(Model.ResearchFields))
                                {
                                    <div class="uk-width-small-1-1 uk-width-medium-1-3 uk-width-large-1-3 uk-width-xLarge-1-3">
                                        <h4 class="heading_c uk-margin-small-bottom silver">فیلدهای مورد علاقه در پژوهش</h4>
                                        @*<ul>
                                                @foreach (var item in Model.ResearchFieldsList)
                                                {
                                                    <li class="white">@item</li>
                                                }
                                            </ul>*@
                                        <div>
                                            @foreach (var item in Model.ResearchFieldsList)
                                            {
                                                <span class="uk-badge uk-badge-success fs-15" style="white-space: initial; margin-top: 3px;">@item</span>
                                            }
                                        </div>
                                    </div>
                                }
                                @if (!string.IsNullOrEmpty(Model.Interests))
                                {
                                    <div class="uk-width-small-1-1 uk-width-medium-1-3 uk-width-large-1-3 uk-width-xLarge-1-3">
                                        <h4 class="heading_c uk-margin-small-bottom silver">علاقه مندی های شخصی</h4>
                                        @*<ul>
                                                @foreach (var item in Model.InterestsList)
                                                {
                                                    <li class="white">@item</li>
                                                }
                                            </ul>*@
                                        <div>
                                            @foreach (var item in Model.InterestsList)
                                            {
                                                <span class="uk-badge uk-badge-success fs-15" style="white-space: initial; margin-top: 3px;">@item</span>
                                            }
                                        </div>
                                    </div>
                                }
                            </div>
                        </text>
                    }

                    @if (!string.IsNullOrEmpty(Model.ResearcherId) ||
!string.IsNullOrEmpty(Model.OrcidId) ||
!string.IsNullOrEmpty(Model.ScopusId) ||
!string.IsNullOrEmpty(Model.GoogleScholarId) ||
!string.IsNullOrEmpty(Model.ResearchGateId) ||
!string.IsNullOrEmpty(Model.PubMedId) ||
!string.IsNullOrEmpty(Model.MedLibId))
                    {
                        <text>
                            <hr class="uk-grid-divider" />
                            <div class="uk-grid uk-margin-medium-top uk-margin-large-bottom" data-uk-grid-margin="">
                                <div class="uk-width-1-1 uk-text-center">
                                    <div id="profile-links" class="md-btn-group mt-15">
                                        @if (!string.IsNullOrEmpty(Model.ScopusId))
                                        {
                                            <a href="@Model.ScopusId" target="_blank" data-uk-tooltip="{pos:'top', cls:'font-arial'}" title="Scopus" class="md-btn md-btn-wave waves-effect waves-button">Scopus</a>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.GoogleScholarId))
                                        {
                                            <a href="@Model.GoogleScholarId" target="_blank" data-uk-tooltip="{pos:'top', cls:'font-arial'}" title="Google Scholar" class="md-btn md-btn-wave waves-effect waves-button">Google Scholar</a>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.ResearcherId))
                                        {
                                            <a href="@Model.ResearcherId" target="_blank" data-uk-tooltip="{pos:'top', cls:'font-arial'}" title="ResearcherID" class="md-btn md-btn-wave waves-effect waves-button">ResearcherID</a>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.ResearchGateId))
                                        {
                                            <a href="@Model.ResearchGateId" target="_blank" data-uk-tooltip="{pos:'top', cls:'font-arial'}" title="Research Gate" class="md-btn md-btn-wave waves-effect waves-button">Research Gate</a>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.OrcidId))
                                        {
                                            <a href="@Model.OrcidId" target="_blank" data-uk-tooltip="{pos:'top', cls:'font-arial'}" title="ORCID" class="md-btn md-btn-wave waves-effect waves-button">ORCID</a>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.PubMedId))
                                        {
                                            <a href="@Model.PubMedId" target="_blank" data-uk-tooltip="{pos:'top', cls:'font-arial'}" title="PubMed" class="md-btn md-btn-wave waves-effect waves-button">PubMed</a>
                                        }
                                        @if (!string.IsNullOrEmpty(Model.MedLibId))
                                        {
                                            <a href="@Model.MedLibId" target="_blank" data-uk-tooltip="{pos:'top', cls:'font-arial'}" title="LibMed" class="md-btn md-btn-wave waves-effect waves-button">LibMed</a>
                                        }
                                    </div>
                                </div>
                            </div>
                        </text>
                    }
                </div>
            </div>
        </div>
    </div>

    @foreach (var section in Model.SectionsOrder)
    {
        if (section.SectionName == SectionName.Bio && Model.IsActiveBio && !string.IsNullOrEmpty(Model.Bio))
        {
            <div class="uk-grid section-in-mobile">
                <div class="uk-width-1-1 uk-container uk-container-center">
                    <div class="icon-container" data-uk-tooltip="{pos:'top', cls:'fs-16p'}" title="بیوگرافی">
                        <i class="material-icons md-48 blue" style="padding-top: 25%;">person</i>
                    </div>
                    <div class="md-card">
                        <div class="md-card-content bg-white black pt-70 bio">
                            @Html.Raw(Model.Bio)
                        </div>
                    </div>
                </div>
            </div>
        }
        else if (section.SectionName == SectionName.IndexAndCharts &&
                ((Model.HasHIndexInformations && Model.ShowHIndexSection) ||
                (Model.HasScopusChart && Model.ShowScopusDocumentsCitationChart) ||
                (Model.HasGoogleChart && Model.ShowGoogleDocumentsCitationChart)))
        {
            <div class="uk-grid uk-margin-medium-top uk-margin-large-bottom section-in-mobile" data-uk-grid-margin="">
                <div class="uk-width-1-1 uk-container uk-container-center">
                    <div class="icon-container" data-uk-tooltip="{pos:'top', cls:'fs-16p'}" title="شاخص ها، ارجاعات و مقالات">
                        <i class="material-icons md-48" style="padding-top: 25%; color:#ff006e;">insert_chart</i>
                    </div>
                    <div class="md-card">
                        <div class="md-card-content pt-70">
                            @if (Model.HasHIndexInformations && Model.ShowHIndexSection)
                            {
                                if (Model.HIndexAndDocumentCitationLastUpdateTime != null)
