@model ContentManagementSystem.Models.ViewModels.ProfileIntroViewModel
@using ContentManagementSystem.DomainClasses
@using ContentManagementSystem.Commons.Web;

@{
    ViewBag.Title = $"صفحه شخصی {Model.Fullname}";
    Layout = MVC.Shared.Views._LayoutProfessorNew;

    var avatarTop = "top: 40px;";
    if (Model.EducationalDegree == "--" && Model.LastUpdateTime == null)
    {
        avatarTop = "top: 85px;";
    }
    else if (Model.EducationalDegree == "--" && Model.LastUpdateTime != null)
    {
        avatarTop = "top: 60px;";
    }
}

<link href="~/build/chartstyles/chart.min.css" rel="stylesheet" />
<div id="sticky-header" data-uk-sticky="{top:-600, animation: 'uk-animation-slide-top'}">
    <div class="uk-grid">
        <div class="uk-width-1-1">
            <div class="uk-block uk-block-primary top-block" style="padding-top: 25px;">
                @*<div class="top-right-actions">
                        <div class="display-inlineblock">
                            <a class="md-fab md-fab-success md-fab-wave-light waves-effect waves-button waves-light small-fab" href="mailto:kiarash.s@hotmail.com"><i class="material-icons"></i></a>
                        </div>
                        <div class="display-inlineblock mr-10">
                            <a class="md-fab md-fab-danger md-fab-wave-light waves-effect waves-button waves-light small-fab" href="mailto:kiarash.s@hotmail.com"><i class="material-icons"></i></a>
                        </div>
                    </div>*@
                <div id="top-spacer" style="max-height: 210px;">
                    <div class="top-name">
                        <div id="top-fullname" class="pb-35">
                            @if (!string.IsNullOrEmpty(Model.Fullname))
                            {
                                <h2 class="white">@Model.Fullname</h2>
                            }
                            @if (Model.EducationalDegree != "--")
                            {
                                <span id="top-educational-degree" class="grey">@Model.EducationalDegree</span>
                            }
                        </div>
                        @if (Model.LastUpdateTime != null)
                        {
                            <div id="last-update" class="silver persian-number" data-uk-tooltip="{pos:'top', cls:'fs-16p'}" title="@Model.JalaiLastUpdateTime">آخرین بروز رسانی صفحه شخصی: @Model.GetRelativeLastUpdateTime</div>
                        }
                    </div>
                    <div class="top-avatar" style="@avatarTop">
                        <img src="@Url.RouteUrl(routeName: "Profile", routeValues: new { action = MVC.Profile.ActionNames.ProfessorAvatar, pageId = ViewContext.RouteData.Values["pageid"].ToString() })" alt="@ViewContext.RouteData.Values["pageid"].ToString()" data-uk-tooltip="{pos:'bottom', cls:'fs-16p'}" title="@Model.Fullname" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@*<div>
        <div class="md-fab-wrapper" style="left:auto; right: 20px;bottom: auto; top: 90px;z-index:100000;">
            <div class="md-fab md-fab-toolbar">
                <i class="material-icons">menu</i>
                <div class="md-fab-toolbar-actions">
                    <a href="javascript:void(0)" data-uk-tooltip="{pos:'top', cls: 'fs-16p'}" title="دروس ارائه شده"><i class="material-icons md-color-red-400"></i></a>
                </div>
            </div>
        </div>
    </div>*@

@if (Model.HasLesson || Model.HasGallery || !string.IsNullOrEmpty(Model.PersianResumeFilename) || !string.IsNullOrEmpty(Model.PersonalWebPage) ||
                                        (!string.IsNullOrEmpty(Model.FreePage) && Model.IsActiveFreePage) || (Model.IsActiveWeeklyProgram && Model.HasWeeklyProgram))
{
    <div class="uk-grid mt-0 section-in-mobile">
        <div class="uk-width-1-1 uk-container uk-container-center">
            <div class="uk-text-center" id="top-buttons">
                @if (Model.IsActiveWeeklyProgram && Model.HasWeeklyProgram)
                {
                    <div class="display-inlineblock tb">
                        <a class="md-fab md-fab-wave-light waves-effect waves-button waves-purple small-fab" href="@Url.RouteUrl("Profile", new { action = MVC.Profile.ActionNames.WeeklyProgram })" data-uk-tooltip="{pos:'bottom', cls:'fs-16p'}" title="برنامه هفتگی"><i class="fa fa-calendar"></i></a>
                    </div>
                }
                @if (!string.IsNullOrEmpty(Model.FreePage) && Model.IsActiveFreePage)
                {
                    <div class="display-inlineblock tb">
                        <a class="md-fab md-fab-wave-light waves-effect waves-button waves-purple small-fab" href="@Url.RouteUrl("Profile", new { action = MVC.Profile.ActionNames.FreePage })" data-uk-tooltip="{pos:'bottom', cls:'fs-16p'}" title="صفحه آزاد"><i class="material-icons">crop_free</i></a>
                    </div>
                }
                @if (Model.HasLesson)
                {
                    <div class="display-inlineblock tb">
                        <a class="md-fab md-fab-wave-light waves-effect waves-button waves-purple small-fab" href="@Url.RouteUrl("Profile", new { action = MVC.Profile.ActionNames.Lessons })" data-uk-tooltip="{pos:'bottom', cls:'fs-16p'}" title="دروس ارائه شده"><i class="fa fa-object-group"></i></a>
                    </div>
                }
                @if (Model.HasGallery)
                {
                    <div class="display-inlineblock tb">
                        <a class="md-fab md-fab-wave-light waves-effect waves-button waves-purple small-fab" href="@Url.RouteUrl("Profile", new { action = MVC.Profile.ActionNames.GalleryList })" data-uk-tooltip="{pos:'bottom', cls:'fs-16p'}" title="گالری"><i class="material-icons">perm_media</i></a>
                    </div>
                }
                @if (!string.IsNullOrEmpty(Model.PersianResumeFilename))
                {
                    <div class="display-inlineblock tb">
                        <a class="md-fab md-fab-success md-fab-wave-light waves-effect waves-button waves-light small-fab" href="@Url.Action(MVC.Home.GetFile(type: 4, fileText: RijndaelManagedEncryption.EncryptRijndael(Model.UserId.ToString() + ";#;" + Model.PersianResumeFilename)))" data-uk-tooltip="{pos:'bottom', cls:'fs-16p'}" title="رزومه"><i class="fa fa-id-card-o"></i></a>
                    </div>
                }
                @if (!string.IsNullOrEmpty(Model.PersonalWebPage))
                {
                    <div class="display-inlineblock tb">
                        <a class="md-fab md-fab-primary md-fab-wave-light waves-effect waves-button waves-light small-fab" href="@Model.PersonalWebPage" target="_blank" rel="nofollow" data-uk-tooltip="{pos:'bottom', cls:'fs-16p'}" title="سایت شخصی"><i class="material-icons">home</i></a>
                    </div>
                }
                @*@if (!string.IsNullOrEmpty(Model.Mobile))
                    {
                        <div class="display-inlineblock tb">
                            <a class="md-fab md-fab-warning md-fab-wave-light waves-effect waves-button waves-light small-fab" href="tel:@Model.Mobile" data-uk-tooltip="{pos:'bottom', cls:'fs-16p'}" title="شماره همراه"><i class="material-icons">phone_iphone</i></a>
                        </div>
                    }*@
            </div>
        </div>
    </div>
}

