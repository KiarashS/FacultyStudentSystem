@{
    ViewBag.Title = "مدیریت اخبار سامانه";
    Layout = MVC.Dashboard.Shared.Views._ProfessorMasterLayout;
}

<div id="page_content">
    <div id="page_content_inner">

        <h3 class="heading_b uk-margin-bottom">مدیریت اخبار سامانه</h3>

        <div class="md-card">
            <div class="md-card-content">
                <div id="news_crud">
                </div>
            </div>
        </div>

    </div>
</div>

<div class="md-fab-wrapper mb-30">
    <a class="md-fab md-fab-accent md-fab-wave-light waves-effect waves-button waves-light" href="#" id="recordAdd" data-uk-tooltip="{pos:'right', cls:'fs-16p'}" title="اضافه کردن خبر جدید">
        <i class="material-icons"></i>
    </a>
</div>

<span id="variables" class="display-hidden" data-list-url="@Url.Action(MVC.Dashboard.News.ActionNames.List, MVC.Dashboard.News.Name)"
      data-create-url="@Url.Action(MVC.Dashboard.News.Create())"
      data-update-url="@Url.Action(MVC.Dashboard.News.Update())"
      data-delete-url="@Url.Action(MVC.Dashboard.News.Delete())"></span>
